charge 
action: START|STOP

charge/limit 
action: ENABLE|DISABLE 
[limit: N] only with ENABLE

charge/schedule 
action: ENABLE|DISABLE
[startTime: HH:MM] only with ENABLE

climate
action: START|STOP
[temperature: C] only with START

engine
action: START|STOP|ON|ACCESSORY_1|ACCESSORY_2

engine/hood
action: OPEN|CLOSE

horn
action: HONK

lights/headlights
action: FLASH

mirrors
action: TILT
mirrors: [{ location, x, y } ... ]

panic
action: START|STOP

ports/charge
action: OPEN|CLOSE

ports/fuel
action: OPEN|CLOSE

security
action: LOCK|UNLOCK

sunroof
action: OPEN|VENT|CLOSE
[percentOpen: N] only can use with OPEN

trunks/front 
action: OPEN|CLOSE

trunks/rear
action: OPEN|CLOSE

windows
action: OPEN|CLOSE|LOCK|UNLOCK
windows: [{location [, percentOpen]}]
